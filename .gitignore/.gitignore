int LED = 12;
int LED2 = 11;
int BUTTON = 4;

void setup() {
  pinMode(LED, OUTPUT);
  pinMode(LED2, OUTPUT);
  pinMode(BUTTON, INPUT);
  digitalWrite(LED, HIGH);
  digitalWrite(LED2, HIGH): 
}

void loop() {
  if(digitalRead(BUTTON) == HIGH)
  digitalWrite(LED, HIGH);
  else
  digitalWrite(LED, LOW);
  
  if(digitalRead(BUTTON) == LOW)
  digitalWrite(LED2, HIGH);
  else
  digitalWrite(LED, LOW);
  
} 
